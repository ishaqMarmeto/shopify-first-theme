{% schema %}
  {
    "name": "custom image with text",
    "class":"custom-image-with-text",
    "tag":"section",
    
    "settings": [
            {
                "type": "image_picker",
                "id": "desktop_image",
                "label": "Desktop image",
                "info": "Recommended size: 1400px  X 400px"
            },
            {
                "type": "image_picker",
                "id": "mobile_image",
                "label": "Mobile image",
                "info": "Recommended size: 400px  X 600px"
            },
            {
                "type": "header",
                "content": "Text customisations",
            },
            {
                "type": "text",
                "id": "heading",
                "label": "Heading",
                "default": "Heading",
            },
            {
                "type": "richtext",
                "id": "description",
                "label": "Description",
                "default": "<p></p>"
            },
            {
                "type": "text",
                "id": "button_label",
                "label": "Button text"
            },
            {
                "type": "url",
                "id": "button_url",
                "label": "Button link"
            },
            {
                "type": "color",
                "id": "button_background",
                "label": "Button background"
            },
        ],
  }
{% endschema %}